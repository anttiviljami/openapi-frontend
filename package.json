{
  "name": "openapi-client-axios",
  "description": "JavaScript client library for consuming OpenAPI-enabled APIs with axios. Types included.",
  "version": "3.2.4",
  "author": "Viljami Kuosmanen <viljami@avoinsorsa.fi>",
  "license": "MIT",
  "keywords": [
    "openapi",
    "swagger",
    "client",
    "axios",
    "frontend",
    "browser",
    "mock",
    "typescript"
  ],
  "homepage": "https://github.com/anttiviljami/openapi-client-axios",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anttiviljami/openapi-client-axios.git"
  },
  "bugs": {
    "url": "https://github.com/anttiviljami/openapi-client-axios/issues"
  },
  "main": "dist/client.standard.node.min.js",
  "browser": "dist/client.standard.es.min.js",
  "types": "dist_tsc/index.d.ts",
  "files": [
    "dist/**/*.js",
    "dist/**/*.map",
    "dist_tsc/**/*.d.ts",
    "!dist/**/typegen*",
    "!*.test.*",
    "!**/*.test.*",
    "!scripts",
    "!node_modules",
    "!src",
    "!*.config.js",
    "!typegen",
    "!rollup*",
    "!uglify-*.js",
    "!jest-*.js",
    "!scripts-*.js",
    "!babelrc-*.js",
    "!.browserlistrc",
    "!.env"
  ],
  "dependencies": {
    "@types/lodash-es": "^4.17.3",
    "axios": "^0.19.0",
    "bath-es5": "^2.1.4",
    "json-schema-deref-sync": "^0.10.0",
    "lodash": "^4.17.11",
    "lodash-es": "^4.17.15",
    "openapi-schema-validation": "^0.4.2",
    "openapi-types": "^1.3.4",
    "query-string": "^6.5.0",
    "swagger-parser": "^8.0.0"
  },
  "devDependencies": {
    "@anttiviljami/dtsgenerator": "^2.0.7",
    "@babel/core": "^7.7.5",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.6",
    "@rollup/plugin-replace": "^2.2.1",
    "@types/jest": "^23.3.14",
    "@types/lodash": "^4.14.121",
    "@types/node": "^10.12.29",
    "@types/swagger-parser": "^4.0.3",
    "@types/yargs": "^13.0.0",
    "axios-mock-adapter": "^1.16.0",
    "babel-jest": "^24.9.0",
    "core-js": "^3.4.8",
    "env-cmd": "^10.0.1",
    "indent-string": "^4.0.0",
    "jest": "^24.8.0",
    "markdown-toc": "^1.2.0",
    "prettier": "^1.16.4",
    "regenerator-runtime": "^0.13.3",
    "rollup": "^1.26.5",
    "rollup-plugin-analyzer": "^3.2.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-ignore-import": "^1.3.2",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "source-map-support": "^0.5.10",
    "ts-jest": "^24.0.2",
    "tslint": "^5.13.1",
    "tslint-microsoft-contrib": "^5.2.1",
    "typescript": "^3.3.3333",
    "uglify-es": "^3.3.9",
    "yargs": "^13.2.2"
  },
  "scripts": {
    "build:all": "npm run build && npm run build:rollup && npm run uglify && npm run release",
    "build": "tsc -p ./ --outDir ./dist_tsc",
    "build:rollup": "rollup -c ./rollup.config.js",
    "release": "node ./scripts-release.js",
    "watch-build": "tsc -w",
    "lint": "tslint --format prose --project .",
    "prettier": "prettier --write src/** __tests__/**",
    "toc": "./scripts/generate-toc.js",
    "prepare": "npm run build:all",
    "test": "env-cmd jest",
    "uglify": "node ./scripts-uglify.js ./uglify-options.js client && node ./scripts-uglify.js ./uglify-options.node.js typegen"
  }
}
